{
  "name": "@presenti/server",
  "version": "1.2.6",
  "description": "Lightweight REST API for retrieving the presence of a set list of Discord users.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:prod": "tsc -p tsconfig.docker.json",
    "watch": "tsc -p . --watch --preserveWatchOutput",
    "prod": "NODE_ENV=production node --experimental-repl-await dist/run.js",
    "dev": "NODE_ENV=development node --experimental-repl-await --trace-deprecation dist/run.js",
    "dev:essync": "SYNCHRONIZE_ELASTIC=1 yarn dev",
    "clean": "rm -rf dist; rm -rf tsconfig.tsbuildinfo",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "docs": "typedoc --out docs src; touch docs/.nojekyll; echo 'docs.presenti.app' > docs/CNAME"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricRabil/PresenceBot.git"
  },
  "keywords": [
    "discordjs",
    "presence",
    "rest",
    "api",
    "http"
  ],
  "author": "Eric Rabil <ericjrabil@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EricRabil/PresenceBot/issues"
  },
  "homepage": "https://github.com/EricRabil/PresenceBot#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "@presenti/auth-client": "^1.2.6",
    "@presenti/client": "^1.2.6",
    "@presenti/core-cache": "^1.2.6",
    "@presenti/logging": "^1.2.6",
    "@presenti/modules": "^1.2.6",
    "@presenti/renderer": "^1.2.6",
    "@presenti/utils": "^1.2.6",
    "@presenti/web": "^1.2.6",
    "@presenti/ws": "^1.2.6",
    "@presenti/shared-db": "^1.2.6",
    "@presenti/shared-infrastructure": "^1.2.6",
    "bcrypt": "^5.0.0",
    "chalk": "^4.0.0",
    "class-validator": "^0.12.2",
    "cookie": "^0.4.1",
    "deep-equal": "^2.0.3",
    "discord.js": "^12.1.1",
    "fs-extra": "^9.0.0",
    "got": "^11.3.0",
    "http-status-codes": "^1.4.0",
    "inquirer": "^7.1.0",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.27",
    "node-fetch": "^3.0.0-beta.7",
    "pg": "^8.0.3",
    "pug": "^2.0.4",
    "sactivity": "^1.0.4",
    "splashy": "^5.0.2",
    "typeorm": "^0.2.24",
    "typescript": "^3.9.6",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v18.3.0",
    "uuid": "^8.2.0",
    "uuid-validate": "^0.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@sendgrid/mail": "^7.2.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.3.3",
    "@types/deep-equal": "^1.0.1",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/ioredis": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^12.12.6",
    "@types/node-fetch": "^2.5.7",
    "@types/pug": "^2.0.4",
    "@types/uuid": "^7.0.2",
    "@types/uuid-validate": "^0.0.1",
    "@types/ws": "^7.2.3",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "ts-node": "^8.10.1",
    "typedoc": "^0.17.6"
