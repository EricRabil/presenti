{
  "name": "@presenti/server",
  "version": "1.0.4",
  "description": "Lightweight REST API for retrieving the presence of a set list of Discord users.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p .",
    "watch": "tsc -p . --watch",
    "prod": "NODE_ENV=production node dist/run.js",
    "dev": "NODE_ENV=development node --experimental-repl-await dist/run.js",
    "docs": "typedoc --out docs src; touch docs/.nojekyll; echo 'docs.presenti.app' > docs/CNAME"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricRabil/PresenceBot.git"
  },
  "keywords": [
    "discordjs",
    "presence",
    "rest",
    "api",
    "http"
  ],
  "author": "Eric Rabil <ericjrabil@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EricRabil/PresenceBot/issues"
  },
  "homepage": "https://github.com/EricRabil/PresenceBot#readme",
  "dependencies": {
    "@presenti/renderer": "^0.1.1",
    "@presenti/utils": "^1.0.9",
    "bcrypt": "^4.0.1",
    "chalk": "^4.0.0",
    "cookie": "^0.4.1",
    "deep-equal": "^2.0.3",
    "discord.js": "^12.1.1",
    "fs-extra": "^9.0.0",
    "got": "^10.7.0",
    "http-status-codes": "^1.4.0",
    "inquirer": "^7.1.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.27",
    "node-fetch": "^2.6.0",
    "pg": "^8.0.3",
    "pug": "^2.0.4",
    "splashy": "^5.0.2",
    "typeorm": "^0.2.24",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v17.4.0",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.3.3",
    "@types/deep-equal": "^1.0.1",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.7",
    "@types/pug": "^2.0.4",
    "@types/uuid": "^7.0.2",
    "@types/winston": "^2.4.4",
    "@types/ws": "^7.2.3",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  }
}
